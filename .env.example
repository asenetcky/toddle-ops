# ToddleOps Environment Configuration
# Copy this file to .env and fill in your actual values

# Ollama Configuration (for local LLM)
OLLAMA_API_BASE="http://localhost:11434"

# Google API Configuration
GOOGLE_API_KEY="your-google-api-key-here"

# Supabase Database Configuration
SUPABASE_PASSWORD="your-supabase-password"
SUPABASE_USER="your-supabase-user"
SUPABASE_CONN_STRING="postgresql://your-user:your-password@your-host:5432/postgres"

# Optional: Override default host and port
# ADK_WEB_HOST=0.0.0.0
# ADK_WEB_PORT=8000
